#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_aa135812c674db57db87d5d75ddd3def/node_modules/astro/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_aa135812c674db57db87d5d75ddd3def/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_aa135812c674db57db87d5d75ddd3def/node_modules/astro/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_aa135812c674db57db87d5d75ddd3def/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.13.2_@netlify+blobs_aa135812c674db57db87d5d75ddd3def/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.13.2_@netlify+blobs_aa135812c674db57db87d5d75ddd3def/node_modules/astro/astro.js" "$@"
fi
