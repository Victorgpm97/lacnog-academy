#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules/vite/bin/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules/vite/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules/vite/bin/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules/vite/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.3.5_@types+node@24.2_ad741c1897df1f0f866bf7ca7ab0de70/node_modules/vite/bin/vite.js" "$@"
fi
