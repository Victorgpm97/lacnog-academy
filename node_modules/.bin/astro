#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_c1d5dfe4afd977b8be5bfba91dae5974/node_modules/astro/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_c1d5dfe4afd977b8be5bfba91dae5974/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_c1d5dfe4afd977b8be5bfba91dae5974/node_modules/astro/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/astro@5.13.2_@netlify+blobs_c1d5dfe4afd977b8be5bfba91dae5974/node_modules:/mnt/d/Archivos Descarga/VSC/CMS/lacnog-academy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.13.2_@netlify+blobs_c1d5dfe4afd977b8be5bfba91dae5974/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.13.2_@netlify+blobs_c1d5dfe4afd977b8be5bfba91dae5974/node_modules/astro/astro.js" "$@"
fi
